{
  "manifest_version": 3,  
  "name": "Snapshot it",
  "description": "Take a snapshot of the selected area on a webpage.",
  "version": "1.0",
  "icons": {
    "16": "logo.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
"action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
